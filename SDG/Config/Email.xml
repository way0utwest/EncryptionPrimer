<?xml version="1.0" encoding="iso-8859-1"?>
<generators>
	<generator
		type="RedGate.SQLDataGenerator.ExpressionGenerators.RegexpGenerator"
		name="Email"
		description="Email Addresses (generated)"
		category="Personal">

		<property name="Expression">[a-z]{4,8}([0-9]{0,3}|\.[a-z]{4,10})@([a-z]{4,9}\.)?([a-z-]{6})\.(org|com|net)</property>

		<matches field="^Email$" score="99" minlen="31"/>
		<matches field="Email" score="90" minlen="31"/>
		<matches field="mail" score="30" minlen="31"/>

		<type type="string"/>
	</generator>
</generators>
